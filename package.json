{
  "name": "vite-project",
  "homepage": "https://alexi182.github.io/pwa-mobile/",
  "description": "Starter kit for modern web applications",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-ts": "tsc -b && vite build",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:eslint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:eslint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "prettify": "prettier --write **/*.{ts,tsx,js,jsx,json}",
    "lint-staged": "lint-staged",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "predeploy": "npm run build",
    "deploy:": "gh-pages -d dist"
  },
  "dependencies": {
    "antd": "^5.25.2",
    "axios": "^1.9.0",
    "gh-pages": "^6.3.0",
    "jwt-decode": "^4.0.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@iflex/iflex-ui": "2.0.1",
    "@types/node": "^24.0.3",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "less": "^4.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^0.12.0"
  }
}
